    <script>
        // Get all result cards
        const allCards = Array.from(document.querySelectorAll('#resultsGrid .info-card'));
        
        // Search functionality
        document.querySelector('.login-btn').addEventListener('click', function() {
            const searchName = document.getElementById('searchName').value.toLowerCase();
            const quirkType = document.getElementById('quirkType').value.toLowerCase();
            const classification = document.getElementById('classification').value.toLowerCase();
            const hereditary = document.getElementById('hereditary').value.toLowerCase();
            
            allCards.forEach(card => {
                const name = card.querySelector('h3').textContent.toLowerCase();
                const quirkNameElement = card.querySelector('p:nth-child(3)');
                const quirkName = quirkNameElement ? quirkNameElement.textContent.toLowerCase() : '';
                const typeElement = card.querySelector('p:nth-child(4)');
                const type = typeElement ? typeElement.textContent.toLowerCase() : '';
                const classElement = card.querySelector('p:nth-child(5)');
                const classText = classElement ? classElement.textContent.toLowerCase() : '';
                const heredElement = card.querySelector('p:nth-child(7)');
                const heredText = heredElement ? heredElement.textContent.toLowerCase() : '';
                
                let matches = true;
                
                // Check name/ID filter
                if (searchName && !name.includes(searchName) && !quirkName.includes(searchName)) {
                    matches = false;
                }
                
                // Check type filter
                if (quirkType && !type.includes(quirkType)) {
                    matches = false;
                }
                
                // Check classification filter
                if (classification && !classText.includes(classification)) {
                    matches = false;
                }
                
                // Check hereditary filter
                if (hereditary) {
                    if (hereditary === 'inherited' && !heredText.includes('inherited')) {
                        matches = false;
                    } else if (hereditary === 'mutation' && !heredText.includes('mutation')) {
                        matches = false;
                    } else if (hereditary === 'combined' && !heredText.includes('combined')) {
                        matches = false;
                    }
                }
                
                // Show or hide card
                card.style.display = matches ? 'block' : 'none';
            });
        });
        
        // Clear filters functionality
        document.querySelectorAll('.login-btn')[1].addEventListener('click', function() {
            document.getElementById('searchName').value = '';
            document.getElementById('quirkType').value = '';
            document.getElementById('classification').value = '';
            document.getElementById('hereditary').value = '';
            
            // Show all cards
            allCards.forEach(card => {
                card.style.display = 'block';
            });
        });
        
        // Sort alphabetically functionality
        document.getElementById('sortAlphabetically').addEventListener('click', function() {
            const resultsGrid = document.getElementById('resultsGrid');
            const cards = Array.from(resultsGrid.querySelectorAll('.info-card'));
            
            // Sort cards alphabetically by name (h3 text)
            cards.sort((a, b) => {
                const nameA = a.querySelector('h3').textContent.trim().toLowerCase();
                const nameB = b.querySelector('h3').textContent.trim().toLowerCase();
                return nameA.localeCompare(nameB);
            });
            
            // Clear and re-append sorted cards
            resultsGrid.innerHTML = '';
            cards.forEach(card => resultsGrid.appendChild(card));
            
            // Update button text to indicate sorted state
            this.textContent = this.textContent === 'Sort A-Z' ? 'Sorted ‚úì' : 'Sort A-Z';
        });
    </script>
