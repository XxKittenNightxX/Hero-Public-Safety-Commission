    <main class="dashboard">
        <div class="container">
            <section class="welcome-section">
                <h2>Quirk Registry Search</h2>
                <p class="mission-statement">
                    Search the comprehensive database of registered quirks. Filter by name, type, classification, 
                    or characteristics to find specific quirk records.
                </p>
            </section>

            <section class="search-section" style="background: rgba(255, 255, 255, 0.05); padding: 30px; border-radius: 12px; margin-bottom: 30px;">
                <h3 style="color: #fff; margin-bottom: 20px;">Search Criteria</h3>
                
                <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); gap: 20px; margin-bottom: 20px;">
                    <div>
                        <label style="display: block; color: #aaa; margin-bottom: 8px;">Name / Registry ID / Quirk Name</label>
                        <input type="text" id="searchName" placeholder="Enter name, ID, or quirk name..." style="width: 100%; padding: 12px; background: rgba(255, 255, 255, 0.1); border: 1px solid rgba(142, 68, 173, 0.4); border-radius: 6px; color: #fff; font-size: 1rem;">
                    </div>
                    
                    <div>
                        <label style="display: block; color: #aaa; margin-bottom: 8px;">Quirk Type</label>
                        <select id="quirkType" style="width: 100%; padding: 12px; background: rgba(255, 255, 255, 0.1); border: 1px solid rgba(142, 68, 173, 0.4); border-radius: 6px; color: #fff; font-size: 1rem;">
                            <option value="">All Types</option>
                            <option value="emitter">Emitter</option>
                            <option value="transformation">Transformation</option>
                            <option value="mutant">Mutant</option>
                            <option value="accumulation">Accumulation</option>
                            <option value="composite">Composite</option>
                        </select>
                    </div>
                    
                    <div>
                        <label style="display: block; color: #aaa; margin-bottom: 8px;">Classification</label>
                        <select id="classification" style="width: 100%; padding: 12px; background: rgba(255, 255, 255, 0.1); border: 1px solid rgba(142, 68, 173, 0.4); border-radius: 6px; color: #fff; font-size: 1rem;">
                            <option value="">All Classifications</option>
                            <option value="common">Common</option>
                            <option value="uncommon">Uncommon</option>
                            <option value="rare">Rare</option>
                            <option value="unique">Unique</option>
                        </select>
                    </div>
                    
                    <div>
                        <label style="display: block; color: #aaa; margin-bottom: 8px;">Hereditary Status</label>
                        <select id="hereditary" style="width: 100%; padding: 12px; background: rgba(255, 255, 255, 0.1); border: 1px solid rgba(142, 68, 173, 0.4); border-radius: 6px; color: #fff; font-size: 1rem;">
                            <option value="">All</option>
                            <option value="inherited">Inherited</option>
                            <option value="mutation">Mutation</option>
                            <option value="combined">Combined</option>
                        </select>
                    </div>
                </div>
                
                <div style="display: flex; gap: 15px;">
                    <button class="login-btn" style="flex: 1;">Search Database</button>
                    <button class="login-btn" style="flex: 1; background: #6c757d;">Clear Filters</button>
                </div>
            </section>

